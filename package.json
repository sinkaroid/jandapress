{
  "name": "jandapress",
  "version": "2.1.7-alpha",
  "description": "RESTful and experimental API for the Doujinshi, Pressing the whole nhentai, pururin, hentaifox, and more.. where the official one is lack.",
  "main": "build/src/index.js",
  "scripts": {
    "build": "rimraf build && tsc",
    "start": "node build/src/index.js",
    "test": "ts-node test/test.ts",
    "test:cf": "ts-node test/nhentaiCookietest.ts",
    "start:prod": "npm run build && node build/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix",
    "build:freshdoc": "rimraf docs && rimraf template && rimraf theme.zip",
    "build:template": " npm run build:freshdoc && curl https://codeload.github.com/ScathachGrip/apidocjs-theme/zip/refs/tags/v9 -o theme.zip && unzip theme.zip && mv apidocjs-theme-9 template",
    "build:apidoc": "npm run build:template && pidoc -i src -o ./docs -t ./template/template-marine",
    "test:nhentai": "start-server-and-test 3000 \"curl -v http://localhost:3000/nhentai/get?book=177013 | jq '.'\"",
    "test:pururin": "start-server-and-test 3000 \"curl -v http://localhost:3000/pururin/get?book=47226 | jq '.'\"",
    "test:hentaifox": "start-server-and-test 3000 \"curl -v http://localhost:3000/hentaifox/get?book=59026 | jq '.'\"",
    "test:asmhentai": "start-server-and-test 3000 \"curl -v http://localhost:3000/asmhentai/get?book=308830 | jq '.'\"",
    "test:hentai2read": "start-server-and-test 3000 \"curl -v http://localhost:3000/hentai2read/get?book=butabako_shotaone_matome_fgo_hen/1 | jq '.'\"",
    "test:simply-hentai": "start-server-and-test 3000 \"curl -v http://localhost:3000/simply-hentai/get?book=fate-grand-order/fgo-sanbunkatsuhou/all-pages | jq '.'\"",
    "test:3hentai": "start-server-and-test 3000 \"curl -v http://localhost:3000/3hentai/get?book=608979 | jq '.'\""
  },
  "apidoc": {
    "title": "Jandapress API Documentation",
    "url": "https://janda.sinkaroid.org",
    "sampleUrl": "https://janda.sinkaroid.org",
    "name": "Jandapress"
  },
  "keywords": [],
  "author": "sinkaroid",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinkaroid/jandapress.git"
  },
  "license": "MIT",
  "dependencies": {
    "@keyv/redis": "^2.5.8",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-slow-down": "^1.6.0",
    "http-cookie-agent": "^5.0.2",
    "keyv": "^4.5.2",
    "phin": "^3.7.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "tough-cookie": "^4.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-slow-down": "^1.3.2",
    "@types/node": "^20.1.4",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "apidoc": "^0.29.0",
    "eslint": "^8.40.0",
    "rimraf": "^5.0.0",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
